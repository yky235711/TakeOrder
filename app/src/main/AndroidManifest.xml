<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yky.takeorder">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FoodActivity" android:theme="@android:style/Theme.NoTitleBar" />
        <activity android:name=".DrinkActivity" android:theme="@android:style/Theme.NoTitleBar" />
        <activity android:name=".MainMenu" android:theme="@android:style/Theme.NoTitleBar" />
        <activity android:name=".SuffixTab1Activity" android:theme="@android:style/Theme.NoTitleBar"/>
        <activity android:name=".SuffixTab2Activity" android:theme="@android:style/Theme.NoTitleBar">
        </activity>
    </application>

</manifest>